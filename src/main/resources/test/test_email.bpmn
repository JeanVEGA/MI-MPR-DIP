          <from xsi:type="tFormalExpression">pririz@fit.cvut.cz</from>
          <to xsi:type="tFormalExpression">_2_FromInput</to>
        </assignment>
      </dataInputAssociation>
    </task>
    <endEvent id="_3" name="End" >
