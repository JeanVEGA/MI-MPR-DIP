  <itemDefinition id="_admissionItem" structureRef="cz.cvut.fit.mi_mpr_dip.admission.domain.Admission" />

  <process processType="Private" isExecutable="true" id="cz.cvut.fit.mi_mpr_dip.admission.2012_decision" name="Sample Process" tns:packageName="defaultPackage" >

    <!-- process variables -->
    <property id="admission" itemSubjectRef="_admissionItem"/>

    <!-- nodes -->
    <startEvent id="_1" name="StartProcess" />
    <endEvent id="_2" name="End" />
    <scriptTask id="_3" name="Decision generate" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Generating decision ...");
if (admission.getAccepted() &amp;&amp; admission.getAppeal()) {
	System.out.println("&gt; ROZHODNUTI O PRIJETI (NA ODVOLANI)");
} else if (admission.getAccepted() &amp;&amp; !admission.getAppeal()) {
	System.out.println("&gt; ROZHODNUTI O PRIJETI");
} else if (!admission.getAccepted() &amp;&amp; admission.getAppeal()) {
	System.out.println("&gt; ROZHODNUTI O NEPRIJETI (NA ODVOLANI)");
} else if (!admission.getAccepted() &amp;&amp; !admission.getAppeal()) {
	System.out.println("&gt; ROZHODNUTI O NEPRIJETI");
}</script>
    </scriptTask>
    <exclusiveGateway id="_4" name="Gateway" gatewayDirection="Converging" />
    <exclusiveGateway id="_6" name="Gateway - appeal possibility check" gatewayDirection="Diverging" />
    <endEvent id="_9" name="End" >
        <terminateEventDefinition/>
    </endEvent>
    <scriptTask id="_10" name="Not accepted" >
      <script></script>
    </scriptTask>
    <userTask id="_11" name="Appeal for decision" >
      <extensionElements>
        <tns:onEntry-script scriptFormat="http://www.java.com/java">
          <script>System.out.println("Possibility of appeal!");
admission.setAppeal(true);</script>
        </tns:onEntry-script>
        <tns:onEntry-script scriptFormat="http://www.java.com/java">
          <script>//if (appealToRector) {
	admission.setAccepted(true);
//}</script>
        </tns:onEntry-script>
      </extensionElements>
      <ioSpecification>
        <inputSet>
        </inputSet>
        <outputSet>
        </outputSet>
      </ioSpecification>
    </userTask>
    <userTask id="_13" name="Appeal approval" >
      <extensionElements>
        <tns:onEntry-script scriptFormat="http://www.java.com/java">
          <script>System.out.println("Appeal approval!");</script>
        </tns:onEntry-script>
        <tns:onEntry-script scriptFormat="http://www.java.com/java">
          <script>//appealToRector = true;</script>
        </tns:onEntry-script>
      </extensionElements>
      <ioSpecification>
        <inputSet>
        </inputSet>
        <outputSet>
        </outputSet>
      </ioSpecification>
    </userTask>
    <task id="_14" name="Decision sent" tns:taskName="Email" >
      <extensionElements>
        <tns:onEntry-script>
          <script>System.out.println("Sent decision ...");</script>
        </tns:onEntry-script>
      </extensionElements>
      <ioSpecification>
        <inputSet>
        </inputSet>
        <outputSet>
        </outputSet>
      </ioSpecification>
    </task>
    <scriptTask id="_15" name="Acceptation check" >
      <script></script>
    </scriptTask>
    <intermediateCatchEvent id="_17" name="Signal" >
      <signalEventDefinition signalRef="expired_time_for_appeal"/>
    </intermediateCatchEvent>
    <exclusiveGateway id="_16" name="Gateway" gatewayDirection="Converging" />

    <!-- connections -->
    <sequenceFlow id="_6-_2" sourceRef="_6" targetRef="_2" name="to_registration" tns:priority="2" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >boolean b = false;
		if (admission.getAccepted()) {
			b = true;
		}
		return b;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_4-_3" sourceRef="_4" targetRef="_3" />
    <sequenceFlow id="_13-_4" sourceRef="_13" targetRef="_4" />
    <sequenceFlow id="_15-_4" sourceRef="_15" targetRef="_4" />
    <sequenceFlow id="_14-_6" sourceRef="_14" targetRef="_6" />
    <sequenceFlow id="_10-_9" sourceRef="_10" targetRef="_9" />
    <sequenceFlow id="_16-_10" sourceRef="_16" targetRef="_10" />
    <sequenceFlow id="_6-_11" sourceRef="_6" targetRef="_11" name="to_appeal" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >boolean b = false;
		if (!admission.getAccepted() &amp;&amp; !admission.getAppeal()) { //&amp;&amp; appealToRector
			b = true;
		}
		return b;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_11-_13" sourceRef="_11" targetRef="_13" />
    <sequenceFlow id="_3-_14" sourceRef="_3" targetRef="_14" />
    <sequenceFlow id="_1-_15" sourceRef="_1" targetRef="_15" />
    <sequenceFlow id="_6-_16" sourceRef="_6" targetRef="_16" name="not_accepted" tns:priority="3" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >return true;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_17-_16" sourceRef="_17" targetRef="_16" />

  </process>

