      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >//Boolean bo = new Boolean(false);
boolean bo = false;
System.out.println("COUNTER: " + admission.getEvaluations().size());
for (Evaluation evaluation : admission.getEvaluations()) {
	String value = evaluation.getValue();
	String type = evaluation.getEvaluationType().getName().toUpperCase(); 
	System.out.println("value: " + value + " type: " + type);
}

System.out.println("bo: " + bo);

return bo;</conditionExpression>
